<body>
<div class="faqs-layout">
	<h1>New FAQ</h1>
	<div class="faqs-content">
	<div>
		<%= bootstrap_form_for(Faq.new) do |f| %>
			<%= f.text_area :body, rows: 5, cols: 25, :class => "tinymce"%>
			<%= f.hidden_field :last_modified_by, :value => current_user.name %>
			<%= f.submit 'Create FAQ', class:'form-control', style: 'width: 400px;'%>
		<% end %>
	</div>
	</div>
	</div>
</body>
<%= tinymce%>
